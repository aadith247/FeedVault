{
  "manifest_version": 3,
  "name": "FeedVault Extension",
  "version": "1.0.0",
  "description": "Save and view your feeds with FeedVault.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": ["storage", "activeTab"],
  "host_permissions": ["<all_urls>"]
} 